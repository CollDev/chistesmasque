<h1>Joke list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Categor√≠a</th>
            <th>Part1</th>
            <th>Part2</th>
            <th>Vote</th>
            <th>Visit</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('joke_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.category.name }}</td>
            <td>{{ entity.part1 }}</td>
            <td>{{ entity.part2 }}</td>
            <td>{{ entity.vote }}</td>
            <td>{{ entity.visit }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('joke_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('joke_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('joke_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
